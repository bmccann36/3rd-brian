{
  "type": "commonjs",
  "name": "3rd-brian",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "esbuild": "^0.25.9",
    "esbuild-plugin-copy": "^2.1.1",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "node esbuild.config.mjs",
    "watch": "tsc -w",
    "watch:bundle": "node esbuild.config.mjs --watch",
    "dev": "tsx watch src/handlers/lambda-handler.ts",
    "dev:node": "nodemon --watch src --ext ts --exec ts-node src/handlers/lambda-handler.ts",
    "start": "node dist/handlers/lambda-handler.js",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"*.{js,json,mjs}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"*.{js,json,mjs}\"",
    "sam:dev": "sam local start-api",
    "sam:build": "sam build"
  },
  "dependencies": {
    "@fastify/aws-lambda": "^6.1.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "fastify": "^5.5.0"
  }
}
